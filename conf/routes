GET     /users                      controllers.UserController.getUsers
GET     /users/:id                  controllers.UserController.getUser(id: Long)

POST    /user                       controllers.UserController.addUser
PUT     /user                       controllers.UserController.updateUser(id: Long)
DELETE  /user                       controllers.UserController.deleteUser(id: Long)

GET     /usernames                  controllers.UserController.getUsernames

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
